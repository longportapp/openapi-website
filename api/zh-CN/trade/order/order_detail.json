                      },
                      "name": {
                        "type": "string",
                        "description": "收费名称"
                      },
                      "amount": {
                        "type": "string",
                        "description": "单项收费金额"
                      },
                      "currency": {
                        "type": "string",
                        "description": "收费货币"
                      }
                    }
                  }
                }
