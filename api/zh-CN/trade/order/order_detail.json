        "request_body": {
          "type": "python",
          "value": "from longport.openapi import TradeContext, Config\n\nconfig = Config.from_env()\nctx = TradeContext(config)\n\nresp = ctx.order_detail(\n    order_id = \"701276261045858304\",\n)\nprint(resp)\n"
