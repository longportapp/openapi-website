      "get": {
        "description": "该接口用于订单详情查询",
        "summary": "订单详情",
        "access": "交易权限",
        "parameters": [
          {
            "name": "order_id",
            "in": "query",
            "description": "订单 ID，用于指定订单 ID 查询，例如：`701276261045858304`",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "订单详情查询成功",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/order_detail_rsp"
                }
              }
            }
          },
          "400": {
            "description": "查询失败，请求参数错误。"
          }
