{
  "name": "openapi-website",
  "version": "0.0.1",
  "license": "MIT",
  "description": "LongPort OpenAPI docs",
  "private": false,
  "scripts": {
    "docusaurus": "docusaurus",
    "dev": "yarn dev:docs",
    "dev:en": "yarn dev:docs --locale en",
    "dev:zh-HK": "yarn dev:docs --locale zh-HK",
    "dev:docs": "cross-env STAGE=dev docusaurus start --port=8000",
    "dev:swagger": "cross-env WATCH=1 node swagger-watch.js",
    "build:swagger": "node swagger-watch.js",
    "build": "yarn clear && yarn build:swagger && yarn build:canary",
    "build:release": "yarn clear && yarn build:swagger && docusaurus build --out-dir dist",
    "build:canary": "yarn clear && yarn build:swagger && docusaurus build --out-dir dist",
    "clear": "docusaurus clear && mkdir -p dist && rm -rf dist",
    "serve": "docusaurus serve --dir dist --port=8008",
    "create:docs": "docusaurus docs:version",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "swizzle": "docusaurus swizzle",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@docusaurus/core": "2.4.0",
    "@docusaurus/preset-classic": "2.4.0",
    "@docusaurus/theme-mermaid": "^2.4.0",
    "@mdx-js/react": "^1.6.22",
    "@saucelabs/theme-github-codeblock": "^0.1.1",
    "ahooks": "^3.3.10",
    "classnames": "^2.3.1",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "prism-react-renderer": "^1.3.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-photo-view": "^1.0.0",
    "tailwindcss": "^3.3.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.4.0",
    "@tsconfig/docusaurus": "^1.0.5",
    "@types/lodash": "^4.17.4",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "docusaurus-plugin-sass": "^0.2.2",
    "js-yaml": "^4.1.0",
    "json-beautify": "^1.1.1",
    "mini-css-extract-plugin": "^2.6.0",
    "node-watch": "^0.7.3",
    "sass": "^1.34.1",
    "typescript": "^4.6.3",
    "widdershins": "^4.0.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
