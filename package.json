{
  "name": "openapi-website",
  "version": "0.0.1",
  "author": "longbridge",
  "module": "index.ts",
  "description": "this is a website of longbridge-openapi",
  "license": "MIT",
  "private": true,
  "scripts": {
    "docs:dev": "bun run --filter './packages/docs' dev",
    "docs:build": "bun run --filter './packages/docs' build",
    "docs:build:canary": "bun run --filter './packages/docs' build:canary",
    "docs:build:prod": "bun run --filter './packages/docs' build:prod",
    "docs:preview": "bun run --filter './packages/docs' preview",
    "lint:docs": "prettier -c --parser typescript \"packages/docs/**/*.{js,ts,vue}\"",
    "format:docs": "prettier --write \"packages/docs/**/*.{js,ts,vue}\""
  },
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@types/bun": "latest",
    "prettier": "^3.5.3"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
